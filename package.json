{
  "name": "fitopolex",
  "version": "1.0.0",
  "description": "first react app",
  "scripts": {
    "build-serve": "parcel src/frontend/index.html",
    "build": "parcel build src/frontend/index.html",
    "start-watch": "nodemon src/backend/index.js",
    "start": "npm run build && node src/backend/index.js",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-serve\"",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
