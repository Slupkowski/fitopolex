{
    "name": "fitopolex",
    "version": "1.0.0",
    "description": "first react app",
    "scripts": {
        "build-serve": "parcel src/frontend/index.html",
        "build": "parcel build src/frontend/index.html",
        "start-watch": "nodemon src/backend/index.js",
        "start": "npm run build && node src/backend/index.js",
        "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-serve\"",
        "format": "prettier \"src/**/*.{js,html}\" --write",
        "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Slupkowski/fitopolex.git"
    },
    "author": "Tomasz SÅ‚upkowski",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Slupkowski/fitopolex/issues"
    },
    "homepage": "https://github.com/Slupkowski/fitopolex#readme",
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.15.14",
        "axios": "^1.6.8",
        "concurrently": "^8.2.2",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.18.3",
        "prettier": "^2.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "buffer": "^6.0.3",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "nodemon": "^3.1.0",
        "parcel": "^2.12.0",
        "process": "^0.11.10",
        "tailwindcss": "^3.4.1"
    }
}
